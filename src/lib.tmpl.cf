:\ ^key 10 = '\ ^if-not* ;
~\ One line comment

:( ^key ~key ) [,] ^= '( ^if-not* ;
~( multilines comment )

~( boolean constant )
:true   ^1 ;
:false  ^0 ;

:on  ^1 swap c! ;
:off ^0 swap c! ;

:hex ^16 base c! ;
:dec ^10 base c! ;
:bin ^2 base c! ;
:base? ^base c@ dup dec . base c! ;

:% ^echo off ;

